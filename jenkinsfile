pipeline {
    agent any 
    stages {
        stage('Compile Stage') { 
            steps {
		withMaven(maven : 'maven_3.5.3'){
		sh 'mvn clean compile'

			}
		}
	}


                
            
        
        stage('Testing Stage') { 
            steps {
	withMaven(maven : 'maven_3.5.3'){
		sh 'mvn test'
                
            }
        }

}

        stage('Deploy') { 
            steps {
		withMaven(maven : 'maven_3.5.3'){
		sh 'mvn deploy'
                }
            }
        }
    }
}
